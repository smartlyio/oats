{
  "name": "@smartlyio/oats-fetch-adapter",
  "version": "7.6.1",
  "license": "MIT",
  "description": "Fetch adapter for Oats a Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist && rm -f ./tmp/*.ts",
    "build": "yarn clean && yarn tsc",
    "lint": "eslint --max-warnings=0 --ext .ts .",
    "lint:fix": "eslint --max-warnings=0 --ext .ts . --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^6.3.0 || ^7.0.0",
    "typescript": "^4.8.0 || ^5.0.0"
