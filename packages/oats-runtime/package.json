{
  "name": "@smartlyio/oats-runtime",
  "version": "4.1.0",
  "license": "MIT",
  "description": "Runtime for Oats a Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/runtime.js",
  "types": "dist/runtime.d.ts",
  "scripts": {
    "clean": "yarn clean:dist && rm -f ./tmp/*.ts",
    "clean:dist": "rm -rf ./dist",
    "prebuild": "yarn clean",
    "build": "yarn tsc",
    "lint": "eslint --max-warnings=0 --ext .ts src test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "^5.0.1",
    "lodash": "^4.17.20"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.22",
    "fast-check": "2.22.0",
    "jest": "27.5.1",
    "jsverify": "0.8.4",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.6.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.6.2"
  }
}
