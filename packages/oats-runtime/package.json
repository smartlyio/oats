{
  "name": "@smartlyio/oats-runtime",
  "version": "3.1.0",
  "license": "MIT",
  "description": "Runtime for Oats a Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/runtime.js",
  "types": "dist/runtime.d.ts",
  "scripts": {
    "clean": "yarn clean:dist && rm -f ./tmp/*.ts",
    "clean:dist": "rm -rf ./dist",
    "prebuild": "yarn clean",
    "build": "yarn tsc",
    "lint": "eslint --max-warnings=0 --ext .ts src test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "^5.0.1",
    "lodash": "^4.17.20"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.168",
    "@types/node": "13.13.51",
    "fast-check": "2.14.0",
    "jest": "26.6.3",
    "jsverify": "0.8.4",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.3.5"
  }
}
