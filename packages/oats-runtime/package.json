{
  "name": "@smartlyio/oats-runtime",
  "version": "5.1.1",
  "license": "MIT",
  "description": "Runtime for Oats a Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/runtime.js",
  "types": "dist/runtime.d.ts",
  "scripts": {
    "clean": "yarn clean:dist && rm -f ./tmp/*.ts",
    "clean:dist": "rm -rf ./dist",
    "prebuild": "yarn clean && yarn tsc --project tsconfig.test.json --noEmit",
    "build": "yarn tsc",
    "lint": "eslint --max-warnings=0 --ext .ts src test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "^5.0.1",
    "@types/encodeurl": "1.0.0",
    "@types/escape-html": "1.0.2",
    "encodeurl": "1.0.2",
    "escape-html": "1.0.3",
    "lodash": "^4.17.20"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.182",
    "@types/node": "16.11.45",
    "fast-check": "2.25.0",
    "jest": "27.5.1",
    "jsverify": "0.8.4",
    "prettier": "2.6.2",
    "ts-jest": "27.1.5",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.4"
  }
}
