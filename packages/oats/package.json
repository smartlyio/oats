{
  "name": "@smartlyio/oats",
  "version": "7.6.3",
  "license": "MIT",
  "description": "Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "pretest": "yarn render",
    "render": "yarn ts-node examples/driver.ts && yarn ts-node render.ts",
    "clean": "rm -rf ./dist/* && rm -rf ./tmp && find test -name '*generated.ts' -exec rm {} \\;",
    "prebuild": "yarn clean",
    "build": "yarn tsc",
    "lint": "eslint --max-warnings=0 --ext .ts src test",
    "lint:fix": "eslint --max-warnings=0 --ext .ts src test --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^6.3.1 || ^7.0.0",
    "typescript": "^4.8.0 || ^5.0.0"
  },
  "dependencies": {
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.20",
    "openapi3-ts": "^3.0.0"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-axios-adapter": "^7.6.3",
    "@smartlyio/oats-fetch-adapter": "^7.6.3",
    "@smartlyio/oats-koa-adapter": "^7.6.3",
    "@smartlyio/oats-runtime": "^7.6.3",
    "@types/jest": "30.0.0",
    "@types/js-yaml": "4.0.5",
    "@types/koa": "3.0.1",
    "@types/koa-mount": "4.0.2",
    "@types/koa-router": "7.4.4",
    "@types/lodash": "4.14.195",
    "@types/node": "24.10.4",
    "axios": "1.8.2",
    "fast-check": "4.5.2",
    "form-data": "4.0.4",
    "jest": "30.2.0",
    "jsverify": "0.8.4",
    "koa": "2.16.2",
    "koa-body": "7.0.1",
    "koa-mount": "4.0.0",
    "koa-router": "14.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.9.2"
  },
  "gitHead": "6afcbb2c8491c594374a82f06c1df5f858ed7681"
