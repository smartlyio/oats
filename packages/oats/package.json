{
  "name": "@smartlyio/oats",
  "version": "3.0.0",
  "license": "MIT",
  "description": "Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "pretest": "yarn render",
    "render": "yarn ts-node examples/driver.ts && yarn ts-node render.ts",
    "clean": "rm -rf ./dist/* && rm -rf ./tmp && find test -name '*generated.ts' -exec rm {} \\;",
    "prebuild": "yarn clean",
    "build": "yarn tsc",
    "lint": "eslint --max-warnings=0 --ext .ts src test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^2.17.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "^5.0.1",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.20",
    "openapi3-ts": "^2.0.0"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-axios-adapter": "^3.0.0",
    "@smartlyio/oats-koa-adapter": "^3.0.0",
    "@smartlyio/oats-runtime": "^3.0.0",
    "@types/jest": "27.0.1",
    "@types/js-yaml": "4.0.1",
    "@types/koa": "2.13.1",
    "@types/koa-mount": "4.0.0",
    "@types/koa-router": "7.4.2",
    "@types/lodash": "4.14.168",
    "@types/node": "14.17.0",
    "axios": "0.21.2",
    "fast-check": "2.14.0",
    "form-data": "4.0.0",
    "jest": "27.0.3",
    "jsverify": "0.8.4",
    "koa": "2.13.1",
    "koa-body": "4.2.0",
    "koa-mount": "4.0.0",
    "koa-router": "10.0.0",
    "ts-jest": "27.0.1",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
  }
}
