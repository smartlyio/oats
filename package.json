{
  "name": "@smartlyio/oats",
  "version": "2.8.0",
  "license": "MIT",
  "description": "Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate:test": "find test -name driver.ts -exec yarn ts-node -r tsconfig-paths/register --project tsconfig.json {} \\;",
    "test": "yarn generate:test && yarn generate:render && yarn jest",
    "generate:render": "yarn ts-node examples/driver.ts",
    "render": "yarn generate:render && yarn ts-node render.ts",
    "clean": "rm -rf ./dist/* && rm -rf ./tmp && find test -name '*generated.ts' -exec rm {} \\;",
    "build": "yarn clean && yarn test && yarn lint && yarn tsc",
    "prepublish": "yarn build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^2.8.0",
    "typescript": "^3.8.3 || ^4.0.0"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "^5.0.1",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.20",
    "openapi3-ts": "^2.0.0"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-axios-adapter": "2.3.0",
    "@smartlyio/oats-koa-adapter": "2.0.2",
    "@smartlyio/oats-runtime": "2.8.0",
    "@types/jest": "26.0.16",
    "@types/js-yaml": "3.12.5",
    "@types/koa": "2.11.6",
    "@types/koa-mount": "4.0.0",
    "@types/koa-router": "7.4.1",
    "@types/lodash": "4.14.165",
    "@types/node": "11.15.21",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "axios": "0.21.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.2.0",
    "fast-check": "2.8.0",
    "form-data": "3.0.0",
    "jest": "26.6.3",
    "jsverify": "0.8.4",
    "koa": "2.13.0",
    "koa-body": "4.2.0",
    "koa-mount": "4.0.0",
    "koa-router": "9.4.0",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.0",
    "typescript": "3.9.7"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "test/.*\\.spec.ts$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null,
    "moduleNameMapper": {
      "^@smartlyio/oats$": "<rootDir>/index"
    }
  }
}
