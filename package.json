{
  "name": "@smartlyio/oats",
  "version": "2.1.0",
  "license": "MIT",
  "description": "Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build-all": "./build-all.sh",
    "test": "yarn ts-node test/runtime-driver.ts && yarn jest",
    "render": "yarn ts-node examples/driver.ts && yarn ts-node render.ts",
    "clean": "rm -rf ./dist/* && rm -f ./tmp/*.ts && rm -f ./runtime/tmp/*.ts",
    "prebuild": "yarn ts-node test/runtime-driver.ts",
    "build": "yarn clean && yarn render && yarn test && yarn lint && yarn tsc",
    "lint": "tslint -c tslint.json --project package.json",
    "fix": "tslint --fix -c tslint.json --project package.json",
    "prepack": "yarn build-all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^2.0.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "^4.0.1",
    "@types/js-yaml": "^3.12.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.4",
    "openapi3-ts": "^1.2.0"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-axios-adapter": "^1.0.1",
    "@smartlyio/oats-koa-adapter": "^2.0.0",
    "@smartlyio/oats-runtime": "^1.0.8",
    "@types/jest": "^25.0.0",
    "@types/koa": "^2.0.50",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.0.40",
    "@types/lodash": "^4.14.140",
    "@types/node": "^11.11.3",
    "axios": "^0.19.0",
    "fast-check": "^1.23.0",
    "form-data": "^3.0.0",
    "jest": "^25.0.0",
    "jsverify": "^0.8.4",
    "koa": "^2.3.0",
    "koa-body": "^4.0.4",
    "koa-mount": "^4.0.0",
    "koa-router": "^8.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^25.0.0",
    "ts-node-dev": "^1.0.0-pre.24",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "test/.*\\.spec.ts$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
