{
  "name": "@smartlyio/oats",
  "version": "1.6.0",
  "license": "MIT",
  "description": "Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "yarn jest test/",
    "render": "yarn ts-node examples/driver.ts && yarn ts-node render.ts",
    "build": "rm -rf ./dist/* && rm -f ./tmp/*.ts && yarn test && yarn lint && yarn render && yarn tsc",
    "lint": "tslint -c tslint.json --project package.json",
    "fix": "tslint --fix -c tslint.json --project package.json",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^1.0.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "^4.0.1",
    "@types/js-yaml": "^3.12.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.4",
    "openapi3-ts": "^1.2.0"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-axios-adapter": "1.0.1",
    "@smartlyio/oats-koa-adapter": "1.0.1",
    "@smartlyio/oats-runtime": "1.0.2",
    "@types/jest": "24.0.18",
    "@types/koa": "2.0.50",
    "@types/koa-mount": "3.0.1",
    "@types/koa-router": "7.0.42",
    "@types/lodash": "4.14.140",
    "@types/node": "11.11.5",
    "axios": "0.19.0",
    "form-data": "2.5.1",
    "jest": "24.9.0",
    "jsverify": "0.8.4",
    "koa": "2.8.2",
    "koa-body": "4.1.1",
    "koa-mount": "3.0.0",
    "koa-router": "7.4.0",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "ts-node-dev": "1.0.0-pre.32",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.6.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "test/.*\\.spec.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
