{
  "name": "@smartlyio/oats",
  "version": "2.5.1",
  "license": "MIT",
  "description": "Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "render": "yarn ts-node examples/driver.ts && yarn ts-node render.ts",
    "clean": "rm -rf ./dist/*",
    "build": "yarn clean && yarn render && yarn jest && yarn lint && yarn tsc",
    "prepublish": "yarn build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^2.4.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "5.0.0",
    "js-yaml": "3.14.0",
    "lodash": "4.17.19",
    "openapi3-ts": "1.4.0"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-axios-adapter": "2.2.0",
    "@smartlyio/oats-koa-adapter": "2.0.0",
    "@smartlyio/oats-runtime": "2.6.1",
    "@types/jest": "26.0.3",
    "@types/js-yaml": "3.12.5",
    "@types/koa": "2.11.3",
    "@types/koa-mount": "4.0.0",
    "@types/koa-router": "7.4.1",
    "@types/lodash": "4.14.157",
    "@types/node": "11.15.16",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "axios": "0.19.2",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.17.1",
    "eslint-plugin-prettier": "3.1.4",
    "fast-check": "1.25.1",
    "form-data": "3.0.0",
    "jest": "26.1.0",
    "jsverify": "0.8.4",
    "koa": "2.13.0",
    "koa-body": "4.2.0",
    "koa-mount": "4.0.0",
    "koa-router": "9.1.0",
    "prettier": "2.0.5",
    "ts-jest": "26.1.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.6"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "test/.*\\.spec.ts$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
