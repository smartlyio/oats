{
  "name": "@smartlyio/oats",
  "version": "2.4.0",
  "license": "MIT",
  "description": "Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "render": "yarn ts-node examples/driver.ts && yarn ts-node render.ts",
    "clean": "rm -rf ./dist/*",
    "build": "yarn clean && yarn render && yarn jest && yarn lint && yarn tsc",
    "prepublish": "yarn build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^2.3.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "^4.0.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.4",
    "openapi3-ts": "^1.2.0"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-axios-adapter": "^2.0.0",
    "@smartlyio/oats-koa-adapter": "^2.0.0",
    "@smartlyio/oats-runtime": "2.3.0",
    "@types/jest": "^26.0.0",
    "@types/js-yaml": "^3.12.2",
    "@types/koa": "^2.0.50",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.0.40",
    "@types/lodash": "^4.14.140",
    "@types/node": "^11.11.3",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "axios": "^0.19.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "fast-check": "^1.23.0",
    "form-data": "^3.0.0",
    "jest": "^26.0.0",
    "jsverify": "^0.8.4",
    "koa": "^2.3.0",
    "koa-body": "^4.0.4",
    "koa-mount": "^4.0.0",
    "koa-router": "^8.0.0",
    "prettier": "^2.0.0",
    "ts-jest": "^25.0.0",
    "ts-node-dev": "^1.0.0-pre.24",
    "typescript": "3.8.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "test/.*\\.spec.ts$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
